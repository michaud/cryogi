{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@components/*": [
                "./src/components/*"
            ],
            "@pages/*": [
                "./src/pages/*"
            ],
            "@contexts": [
                "./src/contexts/*"
            ],
            "@services/*": [
                "./src/services/*"
            ],
            "@layouts/*": [
                "./src/layouts/*"
            ],
            "@utils/*": [
                "./src/utils/*"
            ],
            "@hooks/*": [
                "./src/hooks/*"
            ],
            "@constants/*": [
                "./src/constants/*"
            ],
            "@styles/*": [
                "./src/scss/*"
            ],
        }
    }
}
