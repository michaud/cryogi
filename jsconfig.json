{
    "compilerOptions": {
        "baseUrl": ".",
        "jsx": "react",
        "paths": {
            "@components/*": [
                "./src/components/*"
            ],
            "@pages/*": [
                "./src/pages/*"
            ],
            "@contexts": [
                "./src/contexts/*"
            ],
            "@services/*": [
                "./src/services/*"
            ],
            "@layouts/*": [
                "./src/layouts/*"
            ],
            "@utils/*": [
                "./src/utils/*"
            ],
            "@hooks/*": [
                "./src/hooks/*"
            ],
            "@constants/*": [
                "./src/constants/*"
            ],
            "@styles/*": [
                "./src/scss/*"
            ],
            "@styled/*": [
                "./src/styled/*"
            ],
        }
    },
    "exclude": ["node_modules", "build", "out", "notes","public"]
}
